{
  "name": "@loopback/pubsub",
  "version": "0.0.1",
  "description": "A LoopBack component for pub/sub support.",
  "engines": {
    "node": ">=8.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-pubsub*.tgz dist package *.tsbuildinfo",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-pubsub*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^1.25.1",
    "@loopback/core": "^1.12.1",
    "@types/ioredis": "^4.14.3",
    "debug": "^4.1.1",
    "ioredis": "^4.14.1"
  },
  "devDependencies": {
    "@loopback/build": "^3.0.1",
    "@loopback/testlab": "^1.10.1",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.13",
    "kafka-node": "^5.0.0",
    "p-event": "^4.1.0"
  },
  "keywords": [
    "LoopBack",
    "Pubsub",
    "Publish",
    "Subscribe"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/pubsub"
  }
}
